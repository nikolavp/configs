data.location=/home/nikolavp/.task
dateformat=d/m/Y
dateformat.annotation=d/m/Y
dateformat.holiday=YMD
dateformat.report=d/m/Y

default.command=next -someday -waiting (+home or +uni)
#default.command=next -someday +work
#
report.basic.description=Basic task details
report.basic.columns=id,project,description
report.basic.filter=status:pending limit:page
report.basic.sort=project+,description+




