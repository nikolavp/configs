#!/bin/bash
#This script copies some replays which i watch 
#in Starcraft pretty neat ;). It is assumed that the replays
#are downloaded in Desktop(the default firefox location)
#but you can change that :)
NOW=$(date +%Y%m%d%k%M)
DESTINATION="/home/nikolavp/.wine/drive_c/Starcraft/maps/replays/"
mkdir ${DESTINATION}${NOW}
cd ${DESTINATION}${NOW}
mv ~/Desktop/*.rep .
cd -
